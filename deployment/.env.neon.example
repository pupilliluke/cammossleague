# Neon Database Environment Variables
# Copy this file to .env.neon and update with your actual Neon credentials

# Database Configuration
NEON_DATABASE_URL=postgresql://username:password@ep-xyz-123.us-east-1.aws.neon.tech/cammossleague?sslmode=require
NEON_USERNAME=your-neon-username
NEON_PASSWORD=your-neon-password
NEON_HOST=ep-xyz-123.us-east-1.aws.neon.tech
NEON_DATABASE=cammossleague

# Security
JWT_SECRET=your-super-secret-jwt-key-at-least-64-characters-long-for-production
ADMIN_PASSWORD=your-secure-admin-password

# Application Configuration
SPRING_PROFILES_ACTIVE=neon
SERVER_PORT=8080

# Frontend Configuration
FRONTEND_URL=https://your-frontend-domain.com
CORS_ALLOWED_ORIGINS=https://your-frontend-domain.com,http://localhost:3000

# Email Configuration (optional)
SPRING_MAIL_HOST=smtp.gmail.com
SPRING_MAIL_PORT=587
SPRING_MAIL_USERNAME=your-email@gmail.com
SPRING_MAIL_PASSWORD=your-app-password

# Logging Level
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_CAMMOSSLEAGUE=INFO