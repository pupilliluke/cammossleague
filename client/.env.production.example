# Production Environment Variables for React Client
# Copy this file to .env.production and update with your production API URL

# API Configuration
VITE_API_BASE_URL=https://your-backend-domain.com/api
VITE_APP_NAME=Cam Moss Basketball League
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PWA=false

# Firebase Configuration (if using Firebase auth)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id

# Analytics (optional)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Error Reporting (optional)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id